            sh:severity sh:Violation
        ] ,
        [
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path dcterms:title ;
            sh:severity sh:Violation
        ] ,
        [
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path dcterms:description ;
            sh:severity sh:Violation
        ] ,
        [
            sh:maxCount 1 ;
            sh:node :DateOrDateTimeDataType_Shape ;
            sh:nodeKind sh:Literal ;
            sh:path dcterms:issued ;
            sh:severity sh:Violation
        ] ,
        [
            sh:maxCount 1 ;
