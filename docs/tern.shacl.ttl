# baseURI: https://w3id.org/tern/shacl/tern/
# imports: http://datashapes.org/dash
# imports: http://www.opengis.net/ont/geosparql
# imports: http://www.w3.org/ns/shacl#
# prefix: tern-shacl
PREFIX dcat: <http://www.w3.org/ns/dcat#>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX sh: <http://www.w3.org/ns/shacl#>
PREFIX sosa: <http://www.w3.org/ns/sosa/>
PREFIX ssn: <http://www.w3.org/ns/ssn/>
PREFIX tern-shacl: <https://w3id.org/tern/shacl/tern/>
PREFIX time: <http://www.w3.org/2006/time#>
PREFIX void: <http://rdfs.org/ns/void#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

tern-shacl:
    a owl:Ontology ;
    owl:imports
        <http://datashapes.org/dash> ,
        <http://www.opengis.net/ont/geosparql> ,
        sh: ;
.

tern-shacl:Attribute
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:description "Useful when [rdfs:subClassOf](http://www.w3.org/2000/01/rdf-schema#subClassOf) entailment is enabled and the proximate class type is required to be recorded." ;
            sh:name "type" ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:type
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/RDFDataset> ;
            sh:description "A link to the RDF payload's metadata which this resource was a part of." ;
            sh:nodeKind sh:IRI ;
            sh:path void:inDataset
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "The identifier of the attribute concept. Attribute concepts are usually described with SKOS controlled vocabularies. TERN manages a list of attributes at [http://linked.data.gov.au/def/tern-cv/dd085299-ae86-4371-ae15-61dfa432f924](http://linked.data.gov.au/def/tern-cv/dd085299-ae86-4371-ae15-61dfa432f924)." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/attribute>
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "The direct link to the value either as an IRI or an RDF literal. The simple value is always equivalent to the value captured in rdf:value of the tern:Value instance." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has simple value" ;
            sh:nodeKind sh:IRIOrLiteral ;
            sh:path <https://w3id.org/tern/ontologies/tern/hasSimpleValue>
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Value> ;
            sh:description "A link to a [tern:Value](https://w3id.org/tern/ontologies/tern/Value) instance which encapsulates the value of this Attribute." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has value" ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/hasValue>
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "A link to the individual which this attribute and its value is applied to. Inverse property of [tern:hasAttribute](https://w3id.org/tern/ontologies/tern/hasAttribute)." ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/isAttributeOf>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Attribute> ;
.

tern-shacl:Boolean
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:datatype xsd:boolean ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdf:value
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Boolean> ;
.

tern-shacl:Dataset
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Distribution> ;
            sh:description "An available distribution of the dataset." ;
            sh:name "dataset distribution" ;
            sh:nodeKind sh:IRI ;
            sh:path dcat:distribution
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Dataset> ;
.

tern-shacl:Date
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:datatype xsd:date ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdf:value
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Date> ;
.

tern-shacl:DateTime
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdf:value
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/DateTime> ;
.

tern-shacl:Deployment
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Platform> ;
            sh:maxCount 1 ;
            sh:name "deployed on platform" ;
            sh:nodeKind sh:IRI ;
            sh:path ssn:deployedOnPlatform
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/System> ;
            sh:name "deployed system" ;
            sh:nodeKind sh:IRI ;
            sh:path ssn:deployedSystem
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Attribute> ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/hasAttribute>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Deployment> ;
.

tern-shacl:Distribution
    a sh:NodeShape ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Distribution> ;
.

tern-shacl:FeatureOfInterest
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:name "identifier" ;
            sh:path dcterms:identifier
        ] ,
        [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "type" ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:type
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/RDFDataset> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path void:inDataset
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <http://www.opengis.net/ont/geosparql#Geometry> ;
            sh:name "has geometry" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path <http://www.opengis.net/ont/geosparql#hasGeometry>
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:path rdfs:comment
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Sample> ;
            sh:name "has sample" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:hasSample
        ] ,
        [
            a sh:PropertyShape ;
            sh:name "is feature of interest of" ;
            sh:nodeKind sh:IRI ;
            sh:or (
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/Observation>
                    ]
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/Sampling>
                    ]
                ) ;
            sh:path sosa:isFeatureOfInterestOf
        ] ,
        [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/featureType>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/FeatureOfInterest> ;
.

tern-shacl:Float
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:datatype xsd:double ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path rdf:value
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:double ;
            sh:description "Error of measurement (±)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:path <https://w3id.org/tern/ontologies/tern/uncertainty>
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "The unit of measure of the value. Use [QUDT units of measure vocabulary](http://qudt.org/vocab/unit/)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/unit>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Float> ;
.

tern-shacl:IRI
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path rdf:value
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/IRI> ;
.

tern-shacl:Instant
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "in XSDDate time stamp" ;
            sh:or (
                    [
                        sh:datatype xsd:dateTimeStamp
                    ]
                    [
                        sh:datatype xsd:dateTime
                    ]
                ) ;
            sh:path time:inXSDDateTimeStamp
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Instant> ;
.

tern-shacl:Integer
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:datatype xsd:integer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdf:value
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "Error of measurement (±)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:Literal ;
            sh:or (
                    [
                        sh:datatype xsd:integer
                    ]
                    [
                        sh:datatype xsd:double
                    ]
                ) ;
            sh:path <https://w3id.org/tern/ontologies/tern/uncertainty>
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "The unit of measure of the value. Use [QUDT units of measure vocabulary](http://qudt.org/vocab/unit/)." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/unit>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Integer> ;
.

tern-shacl:ManagedFeature
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:datatype xsd:date ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <https://w3id.org/tern/ontologies/tern/dateCommissioned>
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:date ;
            sh:maxCount 1 ;
            sh:path <https://w3id.org/tern/ontologies/tern/dateDecommissioned>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/ManagedFeature> ;
.

tern-shacl:MaterialSample
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:path <http://rs.tdwg.org/dwc/terms/materialSampleID>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/MaterialSample> ;
.

tern-shacl:Observation
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:name "identifier" ;
            sh:path dcterms:identifier
        ] ,
        [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "type" ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:type
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/RDFDataset> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path void:inDataset
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <http://www.opengis.net/ont/geosparql#Geometry> ;
            sh:name "has geometry" ;
            sh:nodeKind sh:BlankNodeOrIRI ;
            sh:path <http://www.opengis.net/ont/geosparql#hasGeometry>
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:path rdfs:comment
        ] ,
        [
            a sh:PropertyShape ;
            sh:name "was associated with" ;
            sh:nodeKind sh:IRI ;
            sh:or (
                    [
                        sh:class <https://w3id.org/tern/ontologies/org/Person>
                    ]
                    [
                        sh:class <https://w3id.org/tern/ontologies/org/Organization>
                    ]
                ) ;
            sh:path prov:wasAssociatedWith
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/FeatureOfInterest> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has feature of interest" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:hasFeatureOfInterest
        ] ,
        [
            a sh:PropertyShape ;
            sh:and (
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/Value>
                    ]
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/Result>
                    ]
                ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has result" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:hasResult
        ] ,
        [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "has simple result" ;
            sh:nodeKing sh:IRI ;
            sh:path sosa:hasSimpleResult
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Sensor> ;
            sh:maxCount 1 ;
            sh:name "made by sensor" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:madeBySensor
        ] ,
        [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "observed property" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:observedProperty
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Instant> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "phenomenon time" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:phenomenonTime
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:dateTime ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "result time" ;
            sh:path sosa:resultTime
        ] ,
        [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "used procedure" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:usedProcedure
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/SiteVisit> ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/hasSiteVisit>
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "The type of observation." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/observationType>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Observation> ;
.

tern-shacl:ObservationCollection
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/FeatureOfInterest> ;
            sh:description "A relation between an Observation and the entity whose quality was observed, or between an Actuation and the entity whose property was modified, or between an act of Sampling and the entity that was sampled." ;
            sh:maxCount 1 ;
            sh:name "has feature of interest" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:hasFeatureOfInterest
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "Link to a member of a collection of observations that share the same value for one or more of the characteristic properties." ;
            sh:minCount 1 ;
            sh:name "has member" ;
            sh:nodeKind sh:IRI ;
            sh:or (
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/Observation>
                    ]
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/ObservationCollection>
                    ]
                ) ;
            sh:path sosa:hasMember
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/FeatureOfInterest> ;
            sh:description "link to the ultimate feature of interest of an observation or act of sampling. This is useful when the proximate feature of interest is a sample of the ultimate feature of interest, directly or trasntitively." ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:hasUltimateFeatureOfInterest
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Sensor> ;
            sh:description "Relation between an Observation and the Sensor which made the Observations." ;
            sh:maxCount 1 ;
            sh:name "made by sensor" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:madeBySensor
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "Relation linking an Observation to the property that was observed. The ObservableProperty should be a property of the FeatureOfInterest (linked by hasFeatureOfInterest) of this Observation." ;
            sh:maxCount 1 ;
            sh:name "observed property" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:observedProperty
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Instant> ;
            sh:description "The time that the Result of an Observation, Actuation, or Sampling applies to the FeatureOfInterest. Not necessarily the same as the resultTime. May be an interval or an instant, or some other compound temporal entity." ;
            sh:maxCount 1 ;
            sh:name "phenomenon time" ;
            sh:path sosa:phenomenonTime
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:dateTime ;
            sh:description "The result time is the instant of time when the Observation, Actuation or Sampling activity was completed." ;
            sh:maxCount 1 ;
            sh:name "result time" ;
            sh:path sosa:resultTime
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "A relation to link to a re-usable Procedure used in making an Observation, an Actuation, or a Sample, typically through a Sensor, Actuator or Sampler." ;
            sh:minCount 1 ;
            sh:name "used procedure" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:usedProcedure
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/ObservationCollection> ;
.

tern-shacl:Quadrat
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:hasValue <http://linked.data.gov.au/def/tern-cv/4362c8f2-b3cc-4816-b5a2-fb7bb4c0cff5> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "feature type" ;
            sh:nodeKind sh:IRI ;
            sh:path <https://w3id.org/tern/ontologies/tern/featureType>
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Quadrat> ;
.

tern-shacl:RDFDataset
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:description "An entity, such as a person, organisation, or service, that is responsible for making contributions to the dataset. The contributor should be described as an RDF resource, rather than just providing the name as a literal." ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:contributor
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "An entity, such as a person, organisation, or service, that is primarily responsible for creating the dataset. The creator should be described as an RDF resource, rather than just providing the name as a literal." ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:creator
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "A textual description of the dataset." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "description" ;
            sh:path dcterms:description
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:date ;
            sh:description "Date of formal issuance (e.g., publication) of the dataset. The value should be formatted and datatyped as an xsd:date." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "issued" ;
            sh:path dcterms:issued
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "An entity, such as a person, organisation, or service, that is responsible for making the dataset available. The publisher should be described as an RDF resource, rather than just providing the name as a literal." ;
            sh:name "publisher" ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:publisher
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "A related resource from which the dataset is derived. The source should be described as an RDF resource, rather than as a literal." ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:source
        ] ,
        [
            a sh:PropertyShape ;
            sh:datatype xsd:string ;
            sh:description "The name of the dataset." ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:name "title" ;
            sh:path dcterms:title
        ] ,
        [
            a sh:PropertyShape ;
            sh:description "The controlled vocabularies used in this RDF dataset." ;
            sh:name "vocabulary" ;
            sh:nodeKind sh:IRI ;
            sh:path void:vocabulary
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/RDFDataset> ;
.

tern-shacl:Result
    a sh:NodeShape ;
    sh:property [
            a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:maxCount 1 ;
            sh:name "is result of" ;
            sh:nodeKind sh:IRI ;
            sh:or (
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/Observation>
                    ]
                    [
                        sh:class <https://w3id.org/tern/ontologies/tern/Sampling>
                    ]
                ) ;
            sh:path sosa:isResultOf
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Result> ;
.

tern-shacl:Sample
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Sampling> ;
            sh:minCount 1 ;
            sh:name "is result of" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:isResultOf
        ] ,
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/FeatureOfInterest> ;
            sh:minCount 1 ;
            sh:name "is sample of" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:isSampleOf
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Sample> ;
.

tern-shacl:Sampler
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:class <https://w3id.org/tern/ontologies/tern/Sampling> ;
            sh:name "made sampling" ;
            sh:nodeKind sh:IRI ;
            sh:path sosa:madeSampling
        ] ,
        [
            a sh:PropertyShape ;
            sh:minCount 1 ;
            sh:name "implements" ;
            sh:nodeKind sh:IRI ;
            sh:path ssn:implements
        ] ;
    sh:targetClass <https://w3id.org/tern/ontologies/tern/Sampler> ;
.

tern-shacl:Sampling
    a sh:NodeShape ;
    sh:property
        [
            a sh:PropertyShape ;
            sh:maxCount 1 ;
            sh:name "type" ;
            sh:nodeKind sh:IRI ;
            sh:path dcterms:type
        ] ,
