@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix sdo: <https://schema.org/> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tern: <https://w3id.org/tern/ontologies/tern/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Rule: sosa:hasFeatureOfInterest values can satisfy the sample-chain node shape.
<urn:example:sampling-node-1>
    a tern:Sampling ;
    sosa:hasFeatureOfInterest <urn:example:site-1> ;
    sosa:hasResult <urn:example:sample-1> ;
    sosa:usedProcedure <urn:example:method-1> ;
    tern:resultDateTime "2024-01-01T00:00:00Z"^^xsd:dateTime ;
    void:inDataset <urn:example:dataset-1> ;
.

<urn:example:method-1>
    a tern:Method ;
    dcterms:source "https://example.com/method/source"^^xsd:anyURI ;
    skos:prefLabel "Example method" ;
.

<urn:example:sample-1>
    a tern:Sample ;
    sosa:isSampleOf <urn:example:site-1> ;
.

<urn:example:site-1>
    a tern:Site ;
    tern:featureType <https://example.com/site-type> ;
    sdo:spatial _:geom1 ;
    void:inDataset <urn:example:dataset-1> ;
.

_:geom1 a geo:Geometry .

<urn:example:dataset-1>
    a tern:RDFDataset ;
.
