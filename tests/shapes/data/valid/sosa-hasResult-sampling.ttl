@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix geo: <http://www.opengis.net/ont/geosparql#> .
@prefix sdo: <https://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tern: <https://w3id.org/tern/ontologies/tern/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Rule: sosa:hasResult for Sampling requires an IRI of type tern:Site/Sample/MaterialSample.
<urn:example:sampling-hasresult-1>
    a tern:Sampling ;
    sosa:hasFeatureOfInterest <urn:example:feature-5> ;
    sosa:hasResult <urn:example:site-5> ;
    sosa:usedProcedure <urn:example:method-5> ;
    tern:resultDateTime "2024-01-03T00:00:00Z"^^xsd:dateTime ;
    void:inDataset <urn:example:dataset-1> ;
.

<urn:example:method-5>
    a tern:Method ;
    dcterms:source "https://example.com/method/source"^^xsd:anyURI ;
    skos:prefLabel "Example method" ;
.

<urn:example:feature-5>
    a tern:FeatureOfInterest ;
    tern:featureType <https://example.com/feature-type/area> ;
    void:inDataset <urn:example:dataset-1> ;
.

<urn:example:site-5>
    a tern:Site ;
    tern:featureType <https://example.com/site-type/plot> ;
    sdo:spatial _:geom5 ;
.

_:geom5 a geo:Geometry .

<urn:example:dataset-1>
    a tern:RDFDataset ;
.
