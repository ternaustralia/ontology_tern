@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix tern: <https://w3id.org/tern/ontologies/tern/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Rule: tern:hasSiteVisit must be an IRI of type tern:SiteVisit.
<urn:example:sampling-sitevisit-bad>
    a tern:Sampling ;
    sosa:hasFeatureOfInterest <urn:example:feature-8> ;
    sosa:hasResult <urn:example:site-8> ;
    sosa:usedProcedure <urn:example:method-8> ;
    tern:hasSiteVisit "not-an-iri" ;
    tern:resultDateTime "2024-01-04T00:00:00Z"^^xsd:dateTime ;
    void:inDataset <urn:example:dataset-1> ;
.

<urn:example:method-8>
    a tern:Method ;
    dcterms:source "https://example.com/method/source"^^xsd:anyURI ;
    skos:prefLabel "Example method" ;
.

<urn:example:feature-8>
    a tern:FeatureOfInterest ;
    tern:featureType <https://example.com/feature-type/area> ;
    void:inDataset <urn:example:dataset-1> ;
.

<urn:example:site-8>
    a tern:Site ;
    tern:featureType <https://example.com/site-type> ;
.

<urn:example:dataset-1>
    a tern:RDFDataset ;
.
